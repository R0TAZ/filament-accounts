{
  "name": "rotaz/filament-accounts",
  "description": "Laravel authentication and authorization system designed for Filament, focusing on multi-tenant account management.",
  "keywords": [
    "rotazapp",
    "filament-accounts",
    "laravel",
    "socialite",
    "authentication",
    "filament",
    "multi-tenant",
    "account management",
    "binary",
    "subscription",
    "saas"
  ],
  "version": "1.0.2",
  "homepage": "https://github.com/R0TAZ/filament-accounts",
  "license": "MIT",
  "authors": [
    {
      "name": "Rodrigo Souza",
      "email": "dev@rotaz.app.br",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "filament/filament": "^3.2.29",
    "illuminate/console": "^11.36",
    "illuminate/contracts": "^11.36",
    "illuminate/support": "^11.36",
    "laravel/socialite": "^5.12",
    "matomo/device-detector": "^6.1",
    "joinapi/filament-utility": "^1.0"
  },
  "require-dev": {
    "laravel/pint": "^1.14",
    "laravel/sanctum": "^4.0",
    "livewire/livewire": "^3.4.9",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^9.0|^10.0",
    "phpunit/phpunit": "^10.5|^11.5.3"
  },
  "autoload": {
    "psr-4": {
      "Rotaz\\FilamentAccounts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "stubs/app/",
      "Database\\Factories\\": "database/factories/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rotaz\\FilamentAccounts\\FilamentAccountsServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
